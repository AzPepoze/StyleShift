{
	"name": "styleshift",
	"description": "Website customizer tool.",
	"version": "0.8.1",
	"keywords": [
		"styleshift",
		"web",
		"customize",
		"enhance",
		"azpepoze"
	],
	"author": "AzPepoze",
	"license": "MIT",
	"scripts": {
		"dev": "npm-run-all -p dev_css dev_js",
		"dev_js": "node utils/builder-extension.js",
		"dev_css": "sass --watch src/main/styles/setting.scss out/build/style.css --style=compressed --no-source-map",
		"full_build": "sass src/main/styles/setting.scss out/build/style.css --style=compressed --no-source-map && node utils/builder-extension.js --production --once && node utils/builder-modules.js",
		"build_release": "node utils/build_release.js",
		"gen_types": "tsc -p utils/tsconfig_gen_types.json && node utils/gen_types.js",
		"gen_raw_types": "tsc -p utils/tsconfig_gen_types.json",
		"gen_templates": "ts-node utils/gen_template.ts -p ./tsconfig_gen_templates.json"
	},
	"devDependencies": {
		"@types/chrome": "^0.0.268",
		"@types/node": "^22.14.1",
		"archiver": "^7.0.1",
		"azpepoze.node_tools": "^1.0.4",
		"chokidar": "^4.0.3",
		"dts-gen": "^0.10.4",
		"esbuild": "^0.24.2",
		"fs-extra": "^11.3.0",
		"jszip": "^3.10.1",
		"monaco-editor": "^0.52.2",
		"monaco-themes": "^0.4.4",
		"npm-run-all": "^4.1.5",
		"sass": "^1.86.3",
		"ts-loader": "^9.5.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3",
		"undici-types": "^7.8.0"
	}
}
